import pkt_call_binary from './impl/browser/call_binary.jsy'
import pkt_multi from './impl/browser/multi.jsy'

export default function browser_pkt_plugin() ::
  return @: order: -1, subclass(FabricHub) ::
    const data_utils = FabricHub.prototype.data_utils

    const bin_call = pkt_call_binary(data_utils)
    mixinCreateChannel @ bin_call

    const _pkts_ = Object.assign @ {}
      @{} bin_call
      bin_call
      pkt_multi(data_utils)


    Object.assign @ FabricHub.prototype, @{}
      _pkts_, _fromObjPacket: _pkts_.fromObjPacket

