import { direct_stream_plugin } from './stream.jsy'
import { tls_plugin } from './tls.jsy'
import { tcp_plugin } from './tcp.jsy'

export default function nodejs_net_plugin(plugin_options={}) ::
  const pi_stream = direct_stream_plugin @ plugin_options
  const pi_tcp = tcp_plugin @ plugin_options
  const pi_tls = tls_plugin @ plugin_options

  return hub => ::
    pi_stream @ hub
    pi_tcp @ hub
    pi_tls @ hub

