  "browser": "umd/msg-fabric-core.min.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-jsy": "^0.10.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "ec-pem": "^0.17.0",
    "faye-websocket": "^0.11.1",
    "mocha": "^5.0.1",
    "rollup": "^0.56.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-gzip": "^1.3.0",
    "rollup-plugin-jsy-babel": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "uglify-es": "^3.3.9",
    "ws": "^4.1.0"
  },
  "babel": {
    "presets": "jsy/lean"
  },
  "scripts": {
    "clean": "rm cjs/* esm/* umd/*",
    "build": "rollup --config",
    "watch": "npm -s run build -- --watch",
    "prepublish": "npm -s test",
    "pretest": "npm -s run build",
    "test": "npm -s run test:one",
    "test:one": "mocha --require babel-core/register test/unittest/node.js",
    "test:watch": "mocha --watch --require babel-core/register test/unittest/node.js",
    "demo": "babel-node test/manual/server.jsy"
